[SourceDisksFiles]

[SourceDisksNames]

[DeviceList]
%DESCRIPTION%=MC2KUSB, USB\VID_1286&PID_8080
%DESCRIPTION%=MC2KUSB, USB\VID_1286&PID_8080&MI_00
;%DESCRIPTION%=MC2KUSB, USB\VID_1fc9&PID_2004

;------------------------------------------------------------------------------
;  Windows 2000/XP/Vista32 Sections

[MC2KUSB.nt]
include=mdmcpq.inf
CopyFiles=DriverCopyFiles
AddReg=MC2KUSB.nt.AddReg

[DriverCopyFiles]
usbser.sys,,,0x20

[MC2KUSB.nt.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,usbser.sys
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[MC2KUSB.nt.Services]
include=mdmcpq.inf
